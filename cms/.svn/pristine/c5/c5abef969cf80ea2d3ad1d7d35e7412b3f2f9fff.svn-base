<script>
	function conditionFun(){
		var urlStr = 'accessStatisticsController.do?websitetale';
		var siteId = $('#sitIdList option:selected').val();//选中的值
		var catId = $('#catId option:selected').val();//选中的值
		$.ajax({
			type:'post',
			url:urlStr,
			data:{'siteId':siteId,'catId':catId},
			success:function(msg){
				$("#website_table").empty();
				$("#website_table").append(msg);
			},
			error:function(){
				lmAlert("提交通讯失败!");
			}
		});
	}
	$("#siteIdList").on("change",function(){
		var siteId = $('#siteIdList option:selected').val();//选中的值
		var catId = "";//选中的值
		submit_search(siteId,catId);
	});

	$("#catIdList").on("change",function(){
		var siteId = $('#siteIdList option:selected').val();//选中的值
		var catId = $('#catIdList option:selected').val();//选中的值
		submit_search(siteId,catId);
	});



	function submit_search(siteId,catId){
		var urlStr = 'accessStatisticsController.do?accessStatistics&siteId='+siteId+'&catId='+catId;
		changePage(urlStr);
	}

	function exportXlS() {
		alert("导出时间较长请耐心等待...");
		var cUrl = "contentStatisticsController.do?exportXls";
		$.ajax({
			url : cUrl,
			type : 'post',
			cache : false,
			data:{'searchYear':searchYear,'searchMonth':searchMonth},
			success : function(result) {
				var d = $.parseJSON(result);
				if (d.success) {
					var msg = d.msg;
					if("" != msg){
						window.open(msg);
					}else{
						alert("导出失败");
					}
				}else{
					alert("导出失败");
				}
			}
		});
	}
</script>