package com.leimingtech.cms.entity.reply;

/**
 * Created by Administrator on 2017/4/19.
 */
public class ReplyVoBackEntity {

    private String contentTitle;//文章 标题
    private String contentUrl;//文章 url
    private String returnContent;//跟帖 内容
    private String replyTime;//跟帖 时间
    private String returnPerson;//跟帖 人
    private String replyIsCheck;//跟帖 审核状态
    private String replyId;//跟帖ID


    public String getContentTitle() {
        return contentTitle;
    }

    public void setContentTitle(String contentTitle) {
        this.contentTitle = contentTitle;
    }

    public String getContentUrl() {
        return contentUrl;
    }

    public void setContentUrl(String contentUrl) {
        this.contentUrl = contentUrl;
    }

    public String getReturnContent() {
        return returnContent;
    }

    public void setReturnContent(String returnContent) {
        this.returnContent = returnContent;
    }

    public String getReplyTime() {
        return replyTime;
    }

    public void setReplyTime(String replyTime) {
        this.replyTime = replyTime;
    }

    public String getReturnPerson() {
        return returnPerson;
    }

    public void setReturnPerson(String returnPerson) {
        this.returnPerson = returnPerson;
    }

    public String getReplyIsCheck() {
        return replyIsCheck;
    }

    public void setReplyIsCheck(String rplyIsCheck) {
        this.replyIsCheck = rplyIsCheck;
    }

    public String getReplyId() {
        return replyId;
    }

    public void setReplyId(String replyId) {
        this.replyId = replyId;
    }
}
