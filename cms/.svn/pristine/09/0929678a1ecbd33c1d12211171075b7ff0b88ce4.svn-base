<div class="modal hide fade" data-keyboard="false" tabindex="-1" data-backdrop="static" data-focus-on="input:first"
     role="dialog" style="width:600px;" id="image_param_modal">
  <div class="modal-header">
    <h3>编辑模块</h3>
  </div>
  <div class="modal-body form-horizontal" style="max-height:inherit;overflow-y: visible;">
    <form id="imageParamForm">
      <div class="row-fluid">
        <div class="control-group">
          <label class="control-label">图片：</label>
          <div class="controls">
            <input type="text" id="imageUrl" name="imageUrl"/>
            <input type="hidden" id="imageModuleId" name="imageModuleId"/>
            <input type="hidden" id="imageParamsId" name="imageParamsId"/>
            <a class="btn" style="padding:0px !important;width:90px;height: 30px;" id="uploadImage">上传图片</a>
          </div>
        </div>
        <div class="control-group">
          <label class="control-label">大小：</label>
          <div class="controls">
            <input type="text" class="span3" id="imageWidth" name="imageWidth"/>×
            <input type="text" class="span3" id="imageHeight" name="imageHeight"/>px
          </div>
        </div>
        <div class="control-group">
          <label class="control-label">描述：</label>
          <div class="controls">
            <input type="text" id="imageDescription" name="imageDescription"/>
          </div>
        </div>
        <div class="control-group">
          <label class="control-label">链接：</label>
          <div class="controls">
            <input type="text" id="linkUrl" name="linkUrl"/>
          </div>
        </div>
        <div class="control-group">
          <label class="control-label">新窗口打开：</label>
          <div class="controls">
            <label class="checkbox">
              <input type="checkbox" name="blank" value="1"/>
            </label>
          </div>
        </div>
      </div>
    </form>
  </div>
  <div class="modal-footer">
    <a class="btn" href="javascript:saveImageParam()">保存</a>
    <a class="btn" data-dismiss="modal" onclick="closeImageParamModal()">关闭</a>
  </div>
</div>

<script type="text/javascript">

    var initUploadImage = function () {

        if($("#uploadImage").children().length>0){
          return ;
        }
        //图片上传
        var uploadImage = WebUploader.create({
            // 选完文件后，是否自动上传。
            auto: true,
            // swf文件路径
            swf: 'plug-in/ueditor/third-party/webuploader/Uploader.swf',
            // 文件接收服务端。
            server: '${contextpath}/controller.jsp?action=uploadimage',
            // 选择文件的按钮。可选。
            // 内部根据当前运行是创建，可能是input元素，也可能是flash.
            pick: '#uploadImage',
            // 不压缩image, 默认如果是jpeg，文件上传前会压缩一把再上传！
            resize: false,
            // 只允许选择图片文件。
            accept: {
                title: 'Images',
                extensions: 'gif,jpg,jpeg,bmp,png',
                mimeTypes: 'image/gif,image/jpg,image/jpeg,image/bmp,image/png'
            }
        });

        //当有文件添加进来的时候
        uploadImage.on('uploadSuccess', function (file, Object) {
            $("#imageUrl").val(Object.url);
        });
    }
</script>

