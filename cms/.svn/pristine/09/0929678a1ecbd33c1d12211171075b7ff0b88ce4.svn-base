<div class = "modal hide fade"  data-keyboard="false"  data-backdrop="static" role = "dialog" id = "editorModal" style="width:720px;">
  <div class = "modal-header">
    <h3>编辑</h3>
  </div>
  <div class = "modal-body">
    <p>
      <script id="contenteditor" name="contenteditor" type="text/plain" style="height:200px;width:100%;">
      </script>
    </p>
  </div>
  <div class = "modal-footer">
    <a id = "savecontent" class = "btn btn-primary" data-dismiss = "modal">保存</a>
    <a class = "btn closeCKEditor" data-dismiss = "modal">关闭</a>
  </div>
</div>

<div class = "modal hide fade" role = "dialog" style="width:1000px;margin-left:-500px;" id = "editTemplate">
  <div class = "modal-header">
    <h3>源码编辑</h3>
  </div>
  <div class = "modal-body" style="max-height:inherit;">
    <!--提示<p></p>-->
    <p>
      <form id="moduleForm">
      <input type="hidden" name="id" id="id" value=""/>
      <input type="hidden" name="moduleKey" value=""/>
      <textarea class="diy_textarea" name="template"></textarea>
      </form>
    </p>
  </div>
  <div class = "modal-footer">
      <a class = "btn" href="javascript:submitTemplateModule()">提交</a>
      <a class = "btn" onclick="closeTemplateModule()" data-dismiss = "modal">关闭</a>
  </div>
</div>

<div class = "modal hide fade" role = "dialog" style="width:1000px;margin-left:-500px;" id = "downloadModal">
  <div class = "modal-header"> <a class = "close" data-dismiss = "modal">×</a>
    <h3>下载</h3>
  </div>
  <div class = "modal-body" style="max-height:inherit;">
    <p>
      <textarea class="diy_textarea"></textarea>
    </p>
  </div>
  <div class = "modal-footer"> <a class = "btn" data-dismiss = "modal">关闭</a> </div>
</div>

<div class = "modal hide fade" tabindex="-1" data-backdrop="static" data-focus-on="input:first" role = "dialog" style="width:500px;" id = "content_param_modal">
  <div class = "modal-header">
    <h3>编辑模块</h3>
  </div>
  <div class = "modal-body form-horizontal" style="max-height:inherit;overflow-y: visible;">
    <form id="paramForm">
      <div class="row-fluid">
        <div class="control-group">
          <label class="control-label">栏目</label>
          <div class="controls" style="position: relative;">
            <input type="text" id="catName" readonly class="span8 form-control" value="" onclick="showMenu('menuContent','catName');"/>
            <input type="hidden" id="catId" name="catId"/>
            <input type="hidden" id="moduleKey" name="moduleKey" value="">
            <input type="hidden" id="moduleId" name="moduleId" value="">
            <input type="hidden" id="paramsId" name="paramsId" value="">
            <div id="menuContent" class="menuContent" style="display:none; position: absolute;">
              <ul id="treeDemo" class="ztree" style="margin-top:0; width:180px; height: 300px;"></ul>
            </div>
          </div>
        </div>
        <div class="control-group">
          <label class="control-label">缩略图</label>
          <div class="controls">
            <label class="radio">
              <input type="radio" name="image" value="0" id="imageFalse"/>否
            </label>
            <label class="radio">
              <input type="radio" name="image" value="1" id="imageTrue"/>是
            </label>
          </div>
        </div>
        <div class="control-group">
          <label class="control-label">条数</label>
          <div class="controls">
            <input type="number" id="count" name="count" class="span3 form-control" value="10"/>
          </div>
        </div>

      </div>
    </form>
  </div>
  <div class = "modal-footer">
    <a class = "btn" href="javascript:saveContentParam()">保存</a>
    <a class = "btn" data-dismiss = "modal" onclick="closeParamModal()">关闭</a>
  </div>
</div>

<!--分页表单-->
<div class = "modal hide fade" tabindex="-1" data-backdrop="static" data-focus-on="input:first" role = "dialog" style="width:500px;" id = "pagination_param_modal">
  <div class = "modal-header">
    <h3>编辑模块</h3>
  </div>
  <div class = "modal-body form-horizontal" style="max-height:inherit;overflow-y: visible;">
    <form id="paginationParamForm">
      <div class="row-fluid">
        <div class="control-group">
          <label class="control-label">选择演示栏目</label>
          <div class="controls" style="position: relative;">
            <input type="text" id="paginationCatName" readonly class="span8 form-control" value="" onclick="showMenu('paginationCatContent','paginationCatName');"/>
            <input type="hidden" id="paginationCatId" name="catId"/>
            <input type="hidden" id="paginationModuleKey" name="moduleKey" value="">
            <input type="hidden" id="paginationModuleId" name="moduleId" value="">
            <input type="hidden" id="paginationParamsId" name="paramsId" value="">
            <div id="paginationCatContent" class="menuContent" style="display:none; position: absolute;">
              <ul id="paginationCatTree" class="ztree" style="margin-top:0; width:180px; height: 300px;"></ul>
            </div>
          </div>
        </div>
      </div>
    </form>
  </div>
  <div class = "modal-footer">
    <a class = "btn" href="javascript:savePaginationParam()">保存</a>
    <a class = "btn" data-dismiss = "modal" onclick="closeParamModal()">关闭</a>
