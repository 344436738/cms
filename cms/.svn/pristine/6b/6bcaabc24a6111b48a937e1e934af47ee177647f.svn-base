/**创建评论表**/
CREATE TABLE "CMS_COMMENTARY" ("ID" VARCHAR2(32), "USERNAME" VARCHAR2(255), "MEMBERLEVEL" VARCHAR2(255), "CONTENTID" VARCHAR2(32), "COMMENT_TYPE" VARCHAR2(50), "TITLE" VARCHAR2(255), "CONTENT" VARCHAR2(255), "COMMENTARYPERSON" VARCHAR2(255), "PERSONID" VARCHAR2(32), "COMMENTARYTIME" DATE, "QQ" VARCHAR2(255), "CELLPHONE" VARCHAR2(255), "TELEPHONE" VARCHAR2(255), "ISCHECK" VARCHAR2(10), "CREATEDTIME" DATE) ;

   COMMENT ON COLUMN "CMS_COMMENTARY"."ID" IS '主键ID';

   COMMENT ON COLUMN "CMS_COMMENTARY"."USERNAME" IS '用户名';

   COMMENT ON COLUMN "CMS_COMMENTARY"."MEMBERLEVEL" IS '会员级别';

   COMMENT ON COLUMN "CMS_COMMENTARY"."CONTENTID" IS '内容id';

   COMMENT ON COLUMN "CMS_COMMENTARY"."COMMENT_TYPE" IS '评价类型';

   COMMENT ON COLUMN "CMS_COMMENTARY"."TITLE" IS '文章主题';

   COMMENT ON COLUMN "CMS_COMMENTARY"."CONTENT" IS '内容';

   COMMENT ON COLUMN "CMS_COMMENTARY"."COMMENTARYPERSON" IS '评论人';

   COMMENT ON COLUMN "CMS_COMMENTARY"."PERSONID" IS '评论人ID';

   COMMENT ON COLUMN "CMS_COMMENTARY"."COMMENTARYTIME" IS '评论时间';

   COMMENT ON COLUMN "CMS_COMMENTARY"."QQ" IS 'qq';

   COMMENT ON COLUMN "CMS_COMMENTARY"."CELLPHONE" IS '手机';

   COMMENT ON COLUMN "CMS_COMMENTARY"."TELEPHONE" IS '电话';

   COMMENT ON COLUMN "CMS_COMMENTARY"."ISCHECK" IS '审核状态';

   COMMENT ON COLUMN "CMS_COMMENTARY"."CREATEDTIME" IS '创建时间';

   COMMENT ON TABLE "CMS_COMMENTARY"  IS '评论';
REM INSERTING into CMS_COMMENTARY
SET DEFINE OFF;

/**创建跟帖表**/
CREATE TABLE "CMS_REPLY" ("ID" VARCHAR2(32), "REPLYPERSON" VARCHAR2(255), "REPLYID" VARCHAR2(32), "REPLYTIME" DATE, "REPLYCONTENT" VARCHAR2(255), "SITEID" VARCHAR2(32), "SUPPORTCOUNT" NUMBER(*,0) DEFAULT 0, "OPPOSECOUNT" NUMBER(*,0) DEFAULT 0, "ISCHECK" VARCHAR2(10) DEFAULT '0', "AUDITORID" VARCHAR2(32), "AUDITORNAME" VARCHAR2(255), "AUDITORCREATETIME" DATE, "RETURNPERSON" VARCHAR2(255), "RETURNID" VARCHAR2(32), "RETURNCONTENT" VARCHAR2(255), "RETURNTIME" DATE, "COMMENTID" VARCHAR2(32), "RETURNCOUNT" NUMBER(*,0) DEFAULT 0, "REPLYPERSONID" VARCHAR2(32), "CONTENTID" VARCHAR2(32)) ;

   COMMENT ON COLUMN "CMS_REPLY"."ID" IS 'id';

   COMMENT ON COLUMN "CMS_REPLY"."REPLYPERSON" IS '跟帖人';

   COMMENT ON COLUMN "CMS_REPLY"."REPLYID" IS '被回复贴的id';

   COMMENT ON COLUMN "CMS_REPLY"."REPLYTIME" IS '跟帖时间';

   COMMENT ON COLUMN "CMS_REPLY"."REPLYCONTENT" IS '跟帖内容 ';

   COMMENT ON COLUMN "CMS_REPLY"."SITEID" IS '站点Id';

   COMMENT ON COLUMN "CMS_REPLY"."SUPPORTCOUNT" IS '点赞数';

   COMMENT ON COLUMN "CMS_REPLY"."OPPOSECOUNT" IS '贬赞数 ';

   COMMENT ON COLUMN "CMS_REPLY"."ISCHECK" IS '审核状态';

   COMMENT ON COLUMN "CMS_REPLY"."AUDITORID" IS '审核人Id';

   COMMENT ON COLUMN "CMS_REPLY"."AUDITORNAME" IS '审核人';

   COMMENT ON COLUMN "CMS_REPLY"."AUDITORCREATETIME" IS '审核时间';

   COMMENT ON COLUMN "CMS_REPLY"."RETURNPERSON" IS '回复人';

   COMMENT ON COLUMN "CMS_REPLY"."RETURNID" IS '回复Id';

   COMMENT ON COLUMN "CMS_REPLY"."RETURNCONTENT" IS '回复内容';

   COMMENT ON COLUMN "CMS_REPLY"."RETURNTIME" IS '回复时间';

   COMMENT ON COLUMN "CMS_REPLY"."COMMENTID" IS '评论ID';

   COMMENT ON COLUMN "CMS_REPLY"."RETURNCOUNT" IS '回复数量';

   COMMENT ON COLUMN "CMS_REPLY"."REPLYPERSONID" IS '跟帖人id';

   COMMENT ON COLUMN "CMS_REPLY"."CONTENTID" IS '文章ID';
REM INSERTING into CMS_REPLY
SET DEFINE OFF;

